<div class="wrapper">


	<div class="shooter_title"></div>
	<div class="shooter_pic"></div>

	<%= form_tag '/login' do |f| %>
	  
	<p><%= text_field_tag :email, nil,  placeholder: 'email' %></p>
	 
	<p><%= password_field_tag :password, nil,  placeholder: 'password' %></p>
	  

	<p><%= submit_tag "Login" %></p>

	<p><%= link_to "Sign Up To Play", new_user_path %></p> <!-- need to link to sign up page -->

	<% end %>

</div>
